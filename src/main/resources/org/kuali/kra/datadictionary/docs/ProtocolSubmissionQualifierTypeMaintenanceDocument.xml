--><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="ProtocolSubmissionQualifierTypeMaintenanceDocument" parent="ProtocolSubmissionQualifierTypeMaintenanceDocument-parentBean"/>
	<bean id="ProtocolSubmissionQualifierTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.irb.actions.submit.ProtocolSubmissionQualifierType"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="ProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType"/>
			</list>
		</property>
		<!-- 
		<property name="lockingKeys">
			<list>
				<value>affiliationTypeCode</value>
			</list>
		</property>
		-->		<property name="allowsRecordDeletion" value="true" />
		
		
		
		
		<property name="documentTypeName" value="ProtocolSubmissionQualifierTypeMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="ProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType" parent="ProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType-parentBean"/>
	<bean id="ProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Protocol Submission Type Qualifier"/>
		<property name="title" value="Edit Protocol Submission Type Qualifier"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="submissionQualifierTypeCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
