CREATE OR REPLACE VIEW OSP$AWARD_APPROVED_EQUIPMENT
(
"MIT_AWARD_NUMBER",
"SEQUENCE_NUMBER",
"ITEM",
"VENDOR",
"MODEL",
"AMOUNT",
"UPDATE_TIMESTAMP",
"UPDATE_USER"
) AS
SELECT 
AWARD_NUMBER,
SEQUENCE_NUMBER,
ITEM,
VENDOR,
MODEL,
AMOUNT,
UPDATE_TIMESTAMP,
UPDATE_USER
FROM AWARD_APPROVED_EQUIPMENT
/