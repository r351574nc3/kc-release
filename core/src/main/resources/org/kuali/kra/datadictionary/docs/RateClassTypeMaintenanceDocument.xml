<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RateClassTypeMaintenanceDocument" parent="RateClassTypeMaintenanceDocument-parentBean"/>

  <bean id="RateClassTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.budget.rates.RateClassType"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="RateClassTypeMaintenanceDocument-EditRateClassTypes"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>rateClassType</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="RateClassTypeMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="RateClassTypeMaintenanceDocument-EditRateClassTypes" parent="RateClassTypeMaintenanceDocument-EditRateClassTypes-parentBean"/>

  <bean id="RateClassTypeMaintenanceDocument-EditRateClassTypes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Rate Class Types"/>
    <property name="title" value="Edit Rate Class Types"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="rateClassType" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="prefixActivityType" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="sortId"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
  </bean>
</beans>